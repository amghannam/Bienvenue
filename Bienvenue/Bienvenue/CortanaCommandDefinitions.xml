<?xml version="1.0" encoding="utf-8" ?> 
<VoiceCommands xmlns="http://schemas.microsoft.com/voicecommands/1.2">
  <CommandSet xml:lang="en-us" Name="CommandSet">
    <CommandPrefix>Listen Up</CommandPrefix>
    <Example>Quit</Example>
    <Command Name="Exit">
      <Example>Exit, quit</Example>
    <ListenFor>Exit</ListenFor>
    <ListenFor>Quit</ListenFor>
    <Feedback>Exiting...</Feedback>
      <Navigate/>
    </Command>
        <Command Name="Reset">
          <Example>Start over, reset</Example>
          <ListenFor>Start over</ListenFor>
        <ListenFor>Reset</ListenFor>
        <Feedback>Restarting...</Feedback>
          <Navigate/>
        </Command>
          <Command Name="AddCountry">
          <Example>Add a new country</Example>
          <ListenFor>Add a new country</ListenFor>
        <Feedback>Adding country...</Feedback>
          <Navigate/>
        </Command>
		<Command Name="CompareCountries">
			<Example>Compare the United States of America and Jordan</Example>
			<ListenFor>Compare {countryA} and {countryB}</ListenFor>
			<Feedback>Listing differences between {countryA} and {countryB}...</Feedback>
			<Navigate/>
		</Command>
  </CommandSet>
</VoiceCommands>